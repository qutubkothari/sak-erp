generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tenant {
  id String @id @default(cuid())
}

model Plant {
  id String @id @default(cuid())
}

model User {
  id String @id @default(cuid())
}

model Vendor {
  id String @id @default(cuid())
}

model PurchaseOrder {
  id String @id @default(cuid())
}

model UidRegistry {
  id String @id @default(cuid())
}
